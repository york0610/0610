{
  "name": "@netlify/sdk",
  "version": "5.0.2",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">= 18.19.0"
  },
  "bin": {
    "ntli": "./bin.js",
    "netlify-extension": "./bin.js",
    "netlify-integration": "./bin-deprecated.js"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/api/netlify.d.ts",
      "default": "./dist/api/netlify.js"
    },
    "./bin": {
      "default": "./dist/bin/index.js"
    },
    "./commands": {
      "default": "./dist/cli/commands/index.js"
    },
    "./cli-utils": {
      "default": "./dist/cli/util.js"
    },
    "./ui": {
      "types": "./dist/ui/main.d.ts",
      "default": "./dist/ui/main.js"
    },
    "./ui/functions": {
      "types": "./dist/ui/functions.d.ts",
      "default": "./dist/ui/functions.js"
    },
    "./ui/functions/trpc": {
      "types": "./dist/ui/functions-trpc.d.ts",
      "default": "./dist/ui/functions-trpc.js"
    },
    "./ui/functions/trpc/unstable": {
      "types": "./dist/ui/functions-trpc-unstable.d.ts",
      "default": "./dist/ui/functions-trpc-unstable.js"
    },
    "./ui/react": {
      "types": "./dist/ui/react.d.ts",
      "default": "./dist/ui/react.js"
    },
    "./ui/react/unstable": {
      "types": "./dist/ui/react-unstable.d.ts",
      "default": "./dist/ui/react-unstable.js"
    },
    "./ui/react/components": {
      "types": "./dist/ui/react-components.d.ts",
      "default": "./dist/ui/react-components.js"
    },
    "./ui/react/tailwind-config": {
      "types": "./dist/ui/tailwind-config.d.ts",
      "default": "./dist/ui/tailwind-config.js"
    },
    "./ui/react/core.css": {
      "import": "./dist/ui/core.css"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@netlify/types": "^2.1.0",
    "chalk": "^5.2.0",
    "commander": "^12.1.0",
    "esbuild": "^0.25.0",
    "esbuild-plugin-copy": "^2.1.1",
    "escalade": "^3.2.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.1.0",
    "moize": "^6.1.6",
    "normalize-package-data": "^7.0.0",
    "open": "^9.1.0",
    "outdent": "^0.8.0",
    "semver": "^7.7.2",
    "string-argv": "^0.3.2",
    "wretch": "^2.7.0",
    "zod": "^4.0.13",
    "@netlify/sdk--extension-api-client": "3.0.2",
    "@netlify/sdk--ui-functions": "2.0.2",
    "@netlify/sdk--ui-react": "2.0.2",
    "@netlify/sdk--ui-core": "2.0.0"
  },
  "devDependencies": {
    "@netlify/build": "^35.2.7",
    "@netlify/functions": "^5.0.1",
    "@types/chance": "^1.1.5",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.3",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/node": "~20.9.5",
    "@types/normalize-package-data": "^2.4.4",
    "@types/semver": "^7.7.0",
    "@types/uuid": "^9.0.0",
    "chance": "^1.1.11",
    "debug": "^4.4.1",
    "empathic": "^2.0.0",
    "get-bin-path": "^11.0.0",
    "get-port": "^7.1.0",
    "jest-extended": "^3.2.4",
    "lodash.kebabcase": "^4.1.1",
    "msw": "^2.3.0",
    "netlify-cli": "^23.9.4",
    "publint": "^0.2.5",
    "strip-ansi": "^6.0.1",
    "type-fest": "^4.35.0",
    "uuid": "^9.0.0",
    "vitest": "^3.2.4",
    "yaml": "^2.5.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "watch": "pnpm run build:watch",
    "clean": "rm -rf dist/ && rm -f *.tsbuildinfo",
    "lint": "publint",
    "postbuild": "cp ./src/ui/core.css ./dist/ui/core.css",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}