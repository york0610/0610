{
  "name": "@netlify/sdk--ui-react",
  "version": "2.0.2",
  "author": "Netlify, Inc.",
  "description": "A React-based toolkit for building UI extensions on top of the Netlify platform.",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist/**",
    "lib/**"
  ],
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./components": {
      "types": "./dist/components/main.d.ts",
      "default": "./dist/components/main.js"
    },
    "./styles.css": {
      "import": "./dist/assets/main.css"
    },
    "./tailwind-config": {
      "default": "./lib/tailwind.config.ts"
    },
    "./unstable": {
      "default": "./dist/unstable/main.js",
      "types": "./dist/unstable/main.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "Safari >= 15",
      "last 2 Edge versions",
      "last 2 Firefox versions",
      "Firefox ESR",
      "> 1%",
      "not IE 11",
      "not op_mini all"
    ],
    "modern": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "@fullstory/browser": "^2.0.5",
    "@hookform/resolvers": "^3.9.0",
    "@netlify/netlify-design-tokens": "^7.1.0",
    "@netlify/tailwind-config": "^2.1.0",
    "@netlify/ui": "0.0.23",
    "@react-hook/resize-observer": "^2.0.1",
    "escape-string-regexp": "^5.0.0",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "history": "^5.3.0",
    "react-hook-form": "^7.52.0",
    "react-select": "^5.8.0",
    "shiki": "^1.18.0",
    "zod": "^3.23.8",
    "@netlify/sdk--ui-core": "2.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.2",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/builder-vite": "^8.3.6",
    "@storybook/components": "^8.3.6",
    "@storybook/csf": "^0.1.11",
    "@storybook/experimental-addon-test": "^8.3.6",
    "@storybook/icons": "^1.2.12",
    "@storybook/manager-api": "^8.3.6",
    "@storybook/preview-api": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@storybook/types": "^8.3.6",
    "@tanstack/react-query": "^5.62.1",
    "@trpc/client": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@tsconfig/strictest": "^2.0.5",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.1.4",
    "autoprefixer": "^10.4.7",
    "chromatic": "^11.12.6",
    "dedent": "^1.5.3",
    "delay": "^6.0.0",
    "glob": "^10.4.5",
    "lodash.merge": "^4.6.2",
    "msw": "^2.6.5",
    "msw-storybook-addon": "^2.0.4",
    "playwright": "^1.55.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.3.6",
    "tailwindcss": "^3.4.4",
    "typescript-plugin-css-modules": "^5.1.0",
    "utility-types": "^3.11.0",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.1.4",
    "vitest-browser-react": "^0.0.1",
    "@netlify/sdk--ui-functions": "2.0.2"
  },
  "peerDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "watch": "pnpm run build:watch",
    "clean": "rm -rf dist/ storybook-static/ *.tsbuildinfo",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  }
}